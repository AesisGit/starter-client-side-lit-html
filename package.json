{
  "name": "@rxdi/starter-client-lit-html",
  "version": "0.0.387",
  "main": "src/main.ts",
  "scripts": {
    "start": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html",
    "lint": "npx tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "introspect": "gapi schema introspect --collect-documents --collect-types"
  },
  "author": "Kristiyan Tachev",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@rxdi/core": "0.3.9",
    "@rxdi/graphql-client": "0.3.9",
    "@rxdi/lit-html": "0.3.9",
    "@rxdi/router": "0.3.9",
    "@webcomponents/custom-elements": "^1.2.4"
  },
  "browserslist": [
    "last 1 chrome versions"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "cssnano": "^4.1.10",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "rootDir": "./",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "globals": {
      "__DEV__": true
    },
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/src/.*\\.spec.(ts|tsx|js)$",
    "verbose": true,
    "collectCoverage": true
  }
}
